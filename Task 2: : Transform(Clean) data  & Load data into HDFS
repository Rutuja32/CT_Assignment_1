//load first dataset csv file into the variable Dataset_1 
grunt> Clean_Data_1 = load 'hdfs://localhost:9000/Assignment/Input1/Dataset_1.csv'  using org.apache.pig.piggybank.storage.CSVExcelStorage (',','YES_MULTILINE','NOCHANGE','SKIP_INPUT_HEADER')as (id:int, score:int, viewcount:int, body:chararray, owneruserid:int, ownerdisplayname:chararray, title:chararray, tags:chararray, UserId:int, displayname:chararray);

//load second dataset csv file into the variable Dataset_2
grunt> Clean_Data_2 = load 'hdfs://localhost:9000/Assignment/Input1/Dataset_2.csv'  using org.apache.pig.piggybank.storage.CSVExcelStorage (',','YES_MULTILINE','NOCHANGE','SKIP_INPUT_HEADER')as (id:int, score:int, viewcount:int, body:chararray, owneruserid:int, ownerdisplayname:chararray, title:chararray, tags:chararray, UserId:int, displayname:chararray);

//load third dataset csv file into the variable Dataset_3
grunt> Clean_Data_3 = load 'hdfs://localhost:9000/Assignment/Input1/Dataset_3.csv'  using org.apache.pig.piggybank.storage.CSVExcelStorage (',','YES_MULTILINE','NOCHANGE','SKIP_INPUT_HEADER')as (id:int, score:int, viewcount:int, body:chararray, owneruserid:int, ownerdisplayname:chararray, title:chararray, tags:chararray, UserId:int, displayname:chararray);

//load fourth dataset csv file into the variable Dataset_4
grunt> Clean_Data_4 = load 'hdfs://localhost:9000/Assignment/Input1/Dataset_4.csv'  using org.apache.pig.piggybank.storage.CSVExcelStorage (',','YES_MULTILINE','NOCHANGE','SKIP_INPUT_HEADER')as (id:int, score:int, viewcount:int, body:chararray, owneruserid:int, ownerdisplayname:chararray, title:chararray, tags:chararray, UserId:int, displayname:chararray);


//To perform data cleaning remove the newline, tags whitespace etc from the column body and title.
Clean_Data_1= FOREACH Clean_Data_1 GENERATE id, score, viewcount, REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((REPLACE(body,'[\r\n]+','')),'<[^>]*>' , ' '),'[^a-zA-Z\\s\']+',' '),'(?=\\S*[\'])([a-zA-Z\'-]+)',''),'(?<![\\w\\-])\\w(?![\\w\\-])',''),'[ ]{2,}',' ') AS body, owneruserid, ownerdisplayname, REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((REPLACE(title,'[\r\n]+','')),'<[^>]*>' , ' '),'[^a-zA-Z\\s\']+',' '),'(?=\\S*[\'])([a-zA-Z\'-]+)',''),'(?<![\\w\\-])\\w(?![\\w\\-])',''),'[ ]{2,}',' ') AS title, tags, UserId, displayname;
Clean_Data_2= FOREACH Clean_Data_2 GENERATE id, score, viewcount, REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((REPLACE(body,'[\r\n]+','')),'<[^>]*>' , ' '),'[^a-zA-Z\\s\']+',' '),'(?=\\S*[\'])([a-zA-Z\'-]+)',''),'(?<![\\w\\-])\\w(?![\\w\\-])',''),'[ ]{2,}',' ') AS body, owneruserid, ownerdisplayname, REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((REPLACE(title,'[\r\n]+','')),'<[^>]*>' , ' '),'[^a-zA-Z\\s\']+',' '),'(?=\\S*[\'])([a-zA-Z\'-]+)',''),'(?<![\\w\\-])\\w(?![\\w\\-])',''),'[ ]{2,}',' ') AS title, tags, UserId, displayname;
Clean_Data_3= FOREACH Clean_Data_3 GENERATE id, score, viewcount, REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((REPLACE(body,'[\r\n]+','')),'<[^>]*>' , ' '),'[^a-zA-Z\\s\']+',' '),'(?=\\S*[\'])([a-zA-Z\'-]+)',''),'(?<![\\w\\-])\\w(?![\\w\\-])',''),'[ ]{2,}',' ') AS body, owneruserid, ownerdisplayname, REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((REPLACE(title,'[\r\n]+','')),'<[^>]*>' , ' '),'[^a-zA-Z\\s\']+',' '),'(?=\\S*[\'])([a-zA-Z\'-]+)',''),'(?<![\\w\\-])\\w(?![\\w\\-])',''),'[ ]{2,}',' ') AS title, tags, UserId, displayname;
Clean_Data_4= FOREACH Clean_Data_4 GENERATE id, score, viewcount, REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((REPLACE(body,'[\r\n]+','')),'<[^>]*>' , ' '),'[^a-zA-Z\\s\']+',' '),'(?=\\S*[\'])([a-zA-Z\'-]+)',''),'(?<![\\w\\-])\\w(?![\\w\\-])',''),'[ ]{2,}',' ') AS body, owneruserid, ownerdisplayname, REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((REPLACE(title,'[\r\n]+','')),'<[^>]*>' , ' '),'[^a-zA-Z\\s\']+',' '),'(?=\\S*[\'])([a-zA-Z\'-]+)',''),'(?<![\\w\\-])\\w(?![\\w\\-])',''),'[ ]{2,}',' ') AS title, tags, UserId, displayname;

//Store the Clean data set into hdfs & follow same cmd for other dataset as well
grunt> Store Clean_Data_1 into 'hdfs://localhost:9000/Assignment/Input1/Clean_Data_1.csv' using PigStorage(',');
