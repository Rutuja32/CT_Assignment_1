hdoop@rutuja-VirtualBox:~$hdfs dfs -mkdir /Assignment/Input
hdoop@rutuja-VirtualBox:~$hdfs dfs -put '/home/hdoop/Documents/CT_Dataset/CT_Dataset_1.csv' /Assignment/Input
